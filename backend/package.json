    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^6.2.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.3",
    "nodemailer": "^6.9.1",
    "pdfkit": "^0.13.0",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "stripe": "^12.2.0",
    "twilio": "^4.10.0",
    "umzug": "^3.8.2",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
