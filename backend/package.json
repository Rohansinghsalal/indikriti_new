{
  "name": "admin-backend",
  "version": "1.0.0",
  "description": "E-commerce Admin Panel Backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node database/simple-migrator.js && npm run seed && nodemon server.js",
    "test": "jest",
    "migrate": "node database/migrator.js migrate",
    "seed": "node database/seeder.js seed",
    "setup": "npm run migrate && npm run seed",
    "setup-mysql": "node setup-db.js && npm run migrate && npm run seed && npm run fix-db",
    "fix-db": "node fix-db.js",
    "test-mysql": "node test-mysql.js",
    "setup-db": "node setup-database.js",
    "fix-schema": "node fix-database-schema.js",
    "fresh-start": "node start-with-db-setup.js",
    "quick-start": "npm run fix-schema && npm start"
  },
  "dependencies": {
