{
  "name": "admin-panel",
  "version": "1.0.0",
  "description": "Multi-company e-commerce admin panel",
  "main": "start-dev.js",
  "scripts": {
    "start": "node start-dev.js",
    "dev": "node start-dev.js",
    "backend": "cd backend && npm run dev",
    "frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "migrate": "cd backend && node database/migrator.js migrate",
    "seed": "cd backend && node database/seeder.js seed",
    "setup": "npm run migrate && npm run seed",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "admin",
    "dashboard",
    "e-commerce",
    "multi-tenant"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "axios": "^1.11.0",
    "concurrently": "^8.2.2",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1",
    "next": "^15.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "typescript": "^5.8.3"
  }
}
